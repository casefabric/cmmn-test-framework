<process xmlns="http://www.omg.org/spec/CMMN/20151109/MODEL" xmlns:cafienne="org.cafienne" name="invokecaseengine" implementationType="http://www.omg.org/spec/CMMN/ProcessType/Unspecified">
    <input id="_26Ke_baseURL" name="baseURL"/>
    <input id="_ZUs0_caseId" name="caseId"/>
    <input id="_MHwW_caseLastModified" name="caseLastModified"/>
    <output id="_oCRq_responsePayload" name="responsePayload"/>
